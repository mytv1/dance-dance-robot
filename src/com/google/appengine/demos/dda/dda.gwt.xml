<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 0.0.0//EN" "http://google-web-toolkit.googlecode.com/svn/tags/0.0.0/distro-source/core/src/gwt-module.dtd">
<module rename-to='dda'>
  <inherits name='com.google.gwt.user.User' />
  <inherits name="com.google.gwt.resources.Resources" />
  <entry-point class='com.google.appengine.demos.dda.client.Main' />
  <set-property name="user.agent" value="safari" />
  <servlet path="/game_service"
           class="com.google.appengine.demos.dda.server.GameServiceImpl"/>
  <source path="client"/>
  <source path="shared"/>
</module>
